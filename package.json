{
  "name": "zkp2p-client-sdk-root",
  "private": true,
  "version": "0.1.0",
  "description": "Browser-first TypeScript SDK for ZKP2P with peerauth extension integration",
  "license": "MIT",
  "main": "packages/client-sdk/dist/index.cjs",
  "module": "packages/client-sdk/dist/index.mjs",
  "types": "packages/client-sdk/dist/index.d.ts",
  "sideEffects": false,
  "exports": {},
  "scripts": {
    "build": "npm --prefix packages/client-sdk run build",
    "clean": "npm --prefix packages/client-sdk run clean",
    "typecheck": "npm --prefix packages/client-sdk run typecheck",
    "lint": "npm --prefix packages/client-sdk run lint",
    "test": "npm --prefix packages/client-sdk run test",
    "release": "npm --prefix packages/client-sdk run release",
    "docs": "npm --prefix packages/client-sdk run docs",
    "examples:inspect": "tsx examples/node-scripts/inspect-client.ts",
    "examples:get-quote": "tsx examples/node-scripts/get-quote.ts"
  },
  "files": [
    "packages/client-sdk/dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "zkp2p",
    "sdk",
    "browser",
    "extension",
    "peerauth",
    "viem"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkp2p/zkp2p-client-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/zkp2p/zkp2p-client-sdk/issues"
  },
  "homepage": "https://github.com/zkp2p/zkp2p-client-sdk#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@zkp2p/client-sdk": "file:packages/client-sdk",
    "tsx": "^4.20.0",
    "viem": "^2.37.3"
  }
}